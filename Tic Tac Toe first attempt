X_placement = input("X,Input where you want to place ")
if X_placement == "1":
    place_1 = "X"
elif X_placement == "2":
    place_2 = "X"
elif X_placement == "3":
    place_3 = "X"
elif X_placement == "4":
    place_4 = "X"
elif X_placement == "5":
    place_5 = "X"
